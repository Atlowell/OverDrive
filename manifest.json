{
  "manifest_version": 2,

  "name": "OverDrive",

  "version": "0",

  "description": "Manage file permissions in Google Drive",
  
  "key": "",

  "oauth2": {
    "client_id": "706092088663-n0891u5ggoet3b6hp43igci2mt305e2p.apps.googleusercontent.com",
    "scopes": ["https://www.googleapis.com/auth/drive", "profile"]
  },
  
  "icons": {
    "32": "img/icon32.png",
    "64": "img/icon64.png",
    "128": "img/icon128.png",
    "154": "img/icon154.png"
  },

  "page_action": {
    "default_icon": {
      "32": "img/icon32.png",
      "64": "img/icon64.png",
      "128": "img/icon128.png",
      "154": "img/icon154.png"
    },
    "default_title": "OverDrive"
  },

  "background": {
    "scripts": ["js/background.js"]
  },

  "incognito": "not_allowed",

  "offline_enabled": false,

  "permissions": [
    "activeTab",
    "background",
    "cookies",
    "debugger",
    "declarativeContent",
    "identity",
    "storage",
    "tabs",
    "https://*.google.com/*",
    "https://cdnjs.cloudflare.com/*",
    "https://cdnjs.cloudflare.com/ajax/libs/jstree/3.2.1/jstree.min.js",
    "https://apis.google.com/*"
  ],

  "web_accessible_resources": [
    "css/*",
    "fonts/*",
    "img/*",
    "js/*",
    "index.html"
  ],
  
  "content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self'"
}